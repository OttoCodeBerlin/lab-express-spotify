<div class="card" style="width: 25rem;">
  <div class="img-card-wrapper">
    <img src="{{this.images.[1].url}}" class="card-img-top" alt="Album Picture">
  </div>
  <div class="card-body">
    <h3 class="card-title">{{this.name}}</h3>
    <form action="/search" method="GET">
      <a href="/tracks/{{this.id}}" class="btn btn-primary">Show Tracks</a>
    </form>
  </div>
</div>